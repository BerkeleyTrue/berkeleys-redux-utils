{
  "name": "berkeleys-redux-utils",
  "version": "0.2.0",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --ext=js,jsx .",
    "lint:fix": "eslint . --ext=js,jsx --fix",
    "build": "BABEL_ENV=production babel src --out-dir lib",
    "pretest": "npm run lint",
    "test": "nyc ava",
    "test:watch": "ava --watch",
    "test:cover": "nyc report --reporter=html",
    "test:cover:alls": "nyc report --reporter=text-lcov | coveralls",
    "test:cover:open": "open coverage/index.html",
    "format": "prettier '*.js' 'src/**/*' --trailing-comma all --single-quote --write",
    "postformat": "npm run lint:fix",
    "commit": "git-cz",
    "commitmsg": "validate-commit-msg",
    "prepublishOnly": "npm run build"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-freecodecamp"
    }
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "failFast": true
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "invariant": "^2.2.1"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "commitizen": "^2.9.6",
    "coveralls": "^2.13.1",
    "cz-freecodecamp": "^1.0.0",
    "eslint": "^4.5.0",
    "eslint-plugin-class-property": "^1.0.6",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "git+https://github.com/berkeleytrue/eslint-plugin-react.git#feat/rwr",
    "husky": "^0.14.3",
    "nyc": "^11.1.0",
    "prettier": "^1.5.2",
    "validate-commit-msg": "^2.14.0"
  },
  "keywords": [
    "react",
    "redux",
    "redux-actions",
    "types"
  ]
}
